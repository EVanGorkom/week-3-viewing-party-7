<% if current_user %>
    <%= button_to "Log Out", '/logout', method: :delete %>
<% else %>
    <%= button_to "Log In", '/login', method: :get %>
    <%= button_to "Create New User", register_path, method: :get %>
    <section class='existing-users'>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li><%= user.email %></li>
        <% end %>
<% end %>
</section>